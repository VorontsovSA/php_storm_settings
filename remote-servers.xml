<application>
  <component name="RemoteServers">
    <remote-server name="Docker 100sp" type="docker">
      <configuration>
        <option name="apiUrl" value="tcp://localhost:2375" />
        <option name="pathMappings">
          <list>
            <DockerPathMappingImpl>
              <option name="localPath" value="C:\" />
              <option name="virtualMachinePath" value="/c" />
            </DockerPathMappingImpl>
          </list>
        </option>
      </configuration>
    </remote-server>
    <remote-server name="Docker Paygate" type="docker">
      <configuration>
        <option name="apiUrl" value="tcp://localhost:2375" />
        <option name="pathMappings">
          <list>
            <DockerPathMappingImpl>
              <option name="localPath" value="C:\" />
              <option name="virtualMachinePath" value="/c" />
            </DockerPathMappingImpl>
          </list>
        </option>
      </configuration>
    </remote-server>
  </component>
</application>